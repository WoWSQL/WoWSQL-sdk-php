{
    "name": "WOWSQL/WOWSQL-sdk",
    "description": "Official PHP client library for WOWSQL - MySQL Backend-as-a-Service with S3 Storage",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "WOWSQL Team",
            "email": "support@wowsql.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "WOWSQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WOWSQL\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

